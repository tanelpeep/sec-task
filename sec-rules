type=PairWithWindow
ptype=RegExp
pattern=pam_unix\(su:auth\): authentication failure; .*ruser=(\w*).*user=(\w*)
desc=User $1 failed to change ID to $2
action=event %s
ptype2=RegExp
pattern2=pam_unix\(su:session\): session opened for user (\w*) by (\w*)
desc2=User %1 successfully changed ID to %2
action2=logonly
window=10
